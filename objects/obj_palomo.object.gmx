        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// State Endings

switch (self.state) {
    case self.die_state:
        // Destroy collider.
        instance_destroy(self.collider);
        
        // Destroy self.
        instance_destroy();
        break;
    case self.fall_state:
        // Destroy collider.
        instance_destroy(self.collider);
        
        // Destroy self.
        instance_destroy();
        break;
    case self.drown_state:
        // Destroy collider.
        instance_destroy(self.collider);
        
        // Destroy self.
        instance_destroy();
        break;
    case self.chase_state:
        self.state = self.idle_state;
        script_execute(self.mode);
        break;
    case self.attack_1_state:
        script_execute(self.mode);
        break;
    case self.attack_2_state:
        script_execute(self.mode);
        break;
    case self.attack_3_state:
        if (!self.gliding) {
            // Hovering animation is over, so glide to target.
            if (instance_exists(self.target)) {
                self.gliding = true;
    
                // Set fixed glide target.
                self.glide_target_x = self.target.x;
                self.glide_target_y = self.target.y;
                self.glide_dist = point_distance(self.x, self.y, self.glide_target_x, self.glide_target_y) + self.run_spd;
            } else {
                // If there's no target, then just glide down in place.
                self.glide_target_x = x;
                self.glide_target_y = y;
                self.glide_dist = point_distance(self.x, self.y, self.glide_target_x, self.glide_target_y) + self.run_spd;
            }
        } else {
            // Gliding animation is over, so set gliding to false in order to have it deactivated for next attack.
            self.gliding = false;
            script_execute(self.mode);
        }
        break;
    default:
        break;        
}
</string>
