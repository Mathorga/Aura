            <kind>1</kind>
            <string>/// Move Parallax Background

for (i = 0; i &lt; id.layers_num; i++) {
    // Save current background position.
    var cur_bg_x = background_x[i];
    var cur_bg_y = background_y[i];

    // Calculate new background position.
    var bg_x = (id.target_x - (room_width / 2)) / (id.base_ratio - ((id.layers_num - i) * id.ratio_step));
    var bg_y = (id.target_y - (room_height / 2)) / (id.base_ratio - ((id.layers_num - i) * id.ratio_step));

    // Interpolate the position between current and calculated.
    background_x[i] = lerp(cur_bg_x, bg_x, id.spd);
    background_y[i] = lerp(cur_bg_y, bg_y, id.spd);
}
</string>
